{
  "name": "molema-app",
  "version": "0.0.8",
  "type": "module",
  "description": "摸鱼软件 - 全局快捷键显示隐藏，快速查看技术文章",
  "main": "./out/main/index.js",
  "author": "zxb",
  "license": "MIT",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "pack": "pnpm build && electron-builder --dir",
    "dist": "electron-vite build && electron-builder --publish=never",
    "dist:publish": "electron-vite build && electron-builder --publish=always",
    "build:win": "pnpm run build && electron-builder --win --config --publish always",
    "build:mac": "pnpm run build && electron-builder --mac --config --publish always",
    "postinstall": "electron-builder install-app-deps",
    "generate-icons": "electron-icon-builder --input=icons/icon.png --output=build --flatten"
  },
  "build": {
    "appId": "com.moyu.app",
    "productName": "摸了吗",
    "publish": {
      "provider": "github",
      "owner": "zxbdzh",
      "repo": "MoLeMa",
      "releaseType": "release"
    },
    "directories": {
      "output": "release/${version}",
      "buildResources": "build"
    },
    "files": [
      "out/**/*",
      "package.json",
      "build/**/*",
      "node_modules/electron-updater/**/*",
      "app-update.yml"
    ],
    "asar": true,
    "asarUnpack": [
      "node_modules/better-sqlite3/**/*",
      "build/**/*",
      "node_modules/electron-updater/**/*",
      "**/app-update.yml"
    ],
    "extraResources": [
      {
        "from": "app-update.yml",
        "to": "app-update.yml"
      }
    ],
    "win": {
      "icon": "build/icon.ico",
      "artifactName": "${productName}-${version}-setup.${ext}",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@formkit/auto-animate": "^0.9.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react-syntax-highlighter": "^15.5.13",
    "animejs": "^4.2.2",
    "audiobuffer-to-wav": "^1.0.0",
    "better-sqlite3": "^12.5.0",
    "chokidar": "^5.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.7.3",
    "framer-motion": "^12.24.10",
    "gsap": "^3.14.2",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "lodash-es": "^4.17.23",
    "lucide-react": "^0.562.0",
    "phosphor-react": "^1.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.70.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "^3.6.0",
    "remark-gfm": "^4.0.1",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "webdav": "^5.8.0",
    "zod": "^4.3.5",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.2",
    "@types/node": "^25.0.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.23",
    "electron": "39.2.7",
    "electron-builder": "^26.4.0",
    "electron-icon-builder": "^2.0.1",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "5.0.0",
    "postcss": "^8.5.6",
    "prebuild-install": "^7.1.3",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "workspaces": [
    "."
  ]
}
